# the name of the plugin
PLUGIN_NAME = hadoop

# Non-generated sources to be scanned for registration routines
NONGENERATED_REGISTER_CPP_FILES = \
	packet-hadoop.cpp 
	
# Non-generated sources
NONGENERATED_CPP_FILES = \
	$(NONGENERATED_REGISTER_CPP_FILES)

# Headers.
CLEAN_HEADER_FILES = \
packet-hadoop.h

HEADER_FILES = \
	$(CLEAN_HEADER_FILES)

include ../Makefile.common.inc
